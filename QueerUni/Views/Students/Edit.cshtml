@model QueerUni.Models.Student

<h2>Edit @Model.Name's Tracks</h2>

@using (Html.BeginForm())
{
  @Html.EditorFor(model => model.Name)
  @Html.EditorFor(model => model.Email)
  @Html.EditorFor(model => model.Track1)
  @Html.EditorFor(model => model.Track2)
  @Html.EditorFor(model => model.Track3)

    <input type="submit" value="Save Changes" class="button"/>
}
